	url = {http://arxiv.org/abs/1711.09804},
	doi = {10.3847/2041-8213/aa9d87},
	abstract = {In this work, we report the discovery and characterization of PSR J1411+2551, a new binary pulsar discovered in the Arecibo 327 MHz Drift Pulsar Survey. Our timing observations of the radio pulsar in the system span a period of about 2.5 years. This timing campaign allowed a precise measurement of its spin period (62.4 ms) and its derivative (9.6 \${\textbackslash}pm\$ 0.7) \${\textbackslash}times 10{\textasciicircum}\{-20\}{\textbackslash}, {\textbackslash}rm s{\textbackslash}, s{\textasciicircum}\{-1\}\$; from these, we derive a characteristic age of \${\textbackslash}sim 10{\textbackslash},\$Gyr and a surface magnetic field strength of 2.5 \${\textbackslash}times 10{\textasciicircum}\{9\}\$ G. These numbers indicate that this pulsar was mildly recycled by accretion of matter from the progenitor of the companion star. The system has an eccentric (\$e{\textbackslash}, = {\textbackslash}, 0.17\$) 2.61 day orbit. This eccentricity allows a highly significant measurement of the rate of advance of periastron, \${\textbackslash}dot\{{\textbackslash}omega\} = 0.07686 {\textbackslash}pm 0.00046 {\textasciicircum}\{{\textbackslash}circ\}{\textasciitilde}\{{\textbackslash}rm yr\}{\textasciicircum}\{-1\}\$. Assuming general relativity accurately models the orbital motion, this implies a total system mass M = \$2.538 {\textbackslash}pm 0.022 M\_\{{\textbackslash}odot\}\$. The minimum companion mass is \$0.92{\textbackslash}, M\_\{{\textbackslash}odot\}\$ and the maximum pulsar mass is \$1.62{\textbackslash}, M\_\{{\textbackslash}odot\}\$. The large companion mass and the orbital eccentricity suggest that PSR J1411+2551 is a double neutron star system; the lightest known to date including the DNS merger GW 170817. Furthermore, the relatively low orbital eccentricity and small proper motion limits suggest that the second supernova had a relatively small associated kick; this and the low system mass suggest that it was an ultra-stripped supernova.},
	number = {2},
	urldate = {2020-04-09},
	journal = {The Astrophysical Journal},
	author = {Martinez, J. G. and Stovall, K. and Freire, P. C. C. and Deneva, J. S. and Tauris, T. M. and Ridolfi, A. and Wex, N. and Jenet, F. A. and McLaughlin, M. A. and Bagchi, M.},
	month = dec,
	year = {2017},
	note = {arXiv: 1711.09804},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Solar and Stellar Astrophysics, General Relativity and Quantum Cosmology},
	pages = {L29},
	file = {arXiv Fulltext PDF:/Users/emily_ramey/Zotero/storage/PAI6SHPX/Martinez et al. - 2017 - Pulsar J1411+2551\: A Low Mass New Double Neutron S.pdf:application/pdf}
}

