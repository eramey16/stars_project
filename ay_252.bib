	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Ng, C. and Guillemot, L. and Freire, P. C. C. and Kramer, M. and Champion, D. J. and Cognard, I. and Theureau, G. and Barr, E. D.},
