	journal = {arXiv:1912.11031 [astro-ph, physics:nucl-th]},
	author = {Raaijmakers, G. and Greif, S. K. and Riley, T. E. and Hinderer, T. and Hebeler, K. and Schwenk, A. and Watts, A. L. and Nissanke, S. and Guillot, S. and Lattimer, J. M. and Ludlam, R. M.},
