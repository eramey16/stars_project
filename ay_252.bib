@article{raithel_neutron_2017,
	title = {From {Neutron} {Star} {Observables} to the {Equation} of {State}. {II}. {Bayesian} {Inference} of {Equation} of {State} {Pressures}},
	volume = {844},
	issn = {1538-4357},
	url = {https://iopscience.iop.org/article/10.3847/1538-4357/aa7a5a},
	doi = {10.3847/1538-4357/aa7a5a},
	abstract = {One of the key goals of observing neutron stars is to infer the equation of state (EoS) of the cold, ultradense matter in their interiors. Here, we present a Bayesian statistical method of inferring the pressures at ﬁve ﬁxed densities, from a sample of mock neutron star masses and radii. We show that while ﬁve polytropic segments are needed for maximum ﬂexibility in the absence of any prior knowledge of the EoS, regularizers are also necessary to ensure that simple underlying EoS are not over-parameterized. For ideal data with small measurement uncertainties, we show that the pressure at roughly twice the nuclear saturation density, rsat, can be inferred to within 0.3 dex for many realizations of potential sources of uncertainties. The pressures of more complicated EoS with signiﬁcant phase transitions can also be inferred to within ∼30\%. We also ﬁnd that marginalizing the multi-dimensional parameter space of pressure to infer a mass–radius relation can lead to biases of nearly 1 km in radius, toward larger radii. Using the full, ﬁve-dimensional posterior likelihoods avoids this bias.},
	language = {en},
	number = {2},
	urldate = {2020-05-03},
	journal = {The Astrophysical Journal},
	author = {Raithel, Carolyn A. and Özel, Feryal and Psaltis, Dimitrios},
	month = aug,
	year = {2017},
	pages = {156},
	file = {Raithel et al. - 2017 - From Neutron Star Observables to the Equation of S.pdf:/Users/emily_ramey/Zotero/storage/DWPQZTFQ/Raithel et al. - 2017 - From Neutron Star Observables to the Equation of S.pdf:application/pdf}
}

