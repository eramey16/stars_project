	author = {Ferdman, Robert D. and Collaboration, the PALFA},
	month = sep,
	year = {2017},
	keywords = {(stars:) binaries (including multiple): close, (stars:) pulsars: general (stars:) pulsars: individual (PSR J1913+1102), gravitation, relativity, stars: neutron},
	pages = {146--149},
	file = {Full Text PDF:/Users/emily_ramey/Zotero/storage/7PECTH7P/Ferdman and Collaboration - 2017 - PSR J1913+1102\: a pulsar in a highly asymmetric an.pdf:application/pdf;Snapshot:/Users/emily_ramey/Zotero/storage/EZH4B7Z5/Ferdman and Collaboration - 2017 - PSR J1913+1102\: a pulsar in a highly asymmetric an.html:text/html}
}

@article{linares_peering_2018,
