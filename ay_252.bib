}

@article{watts_probing_2014,
	title = {Probing the neutron star interior and the {Equation} of {State} of cold dense matter with the {SKA}},
	url = {http://arxiv.org/abs/1501.00042},
	abstract = {With an average density higher than the nuclear density, neutron stars (NS) provide a unique test-ground for nuclear physics, quantum chromodynamics (QCD), and nuclear superfluidity. Determination of the fundamental interactions that govern matter under such extreme conditions is one of the major unsolved problems of modern physics, and -- since it is impossible to replicate these conditions on Earth -- a major scientific motivation for SKA. The most stringent observational constraints come from measurements of NS bulk properties: each model for the microscopic behaviour of matter predicts a specific density-pressure relation (its `Equation of state', EOS). This generates a unique mass-radius relation which predicts a characteristic radius for a large range of masses and a maximum mass above which NS collapse to black holes. It also uniquely predicts other bulk quantities, like maximum spin frequency and moment of inertia. The SKA, in Phase 1 and particularly in Phase 2 will, thanks to the exquisite timing precision enabled by its raw sensitivity, and surveys that dramatically increase the number of sources: 1) Provide many more precise NS mass measurements (high mass NS measurements are particularly important for ruling out EOS models); 2) Allow the measurement of the NS moment of inertia in highly relativistic binaries such as the Double Pulsar; 3) Greatly increase the number of fast-spinning NS, with the potential discovery of spin frequencies above those allowed by some EOS models; 4) Improve our knowledge of new classes of binary pulsars such as black widows and redbacks (which may be massive as a class) through sensitive broad-band radio observations; and 5) Improve our understanding of dense matter superfluidity and the state of matter in the interior through the study of rotational glitches, provided that an ad-hoc campaign is developed.},
	urldate = {2020-04-17},
	journal = {arXiv:1501.00042 [astro-ph, physics:nucl-th]},
	author = {Watts, Anna and Xu, Renxin and Espinoza, Cristobal and Andersson, Nils and Antoniadis, John and Antonopoulou, Danai and Buchner, Sarah and Dai, Shi and Demorest, Paul and Freire, Paulo and Hessels, Jason and Margueron, Jerome and Oertel, Micaela and Patruno, Alessandro and Possenti, Andrea and Ransom, Scott and Stairs, Ingrid and Stappers, Ben},
	month = dec,
	year = {2014},
	note = {arXiv: 1501.00042},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Solar and Stellar Astrophysics, Nuclear Theory},
	file = {arXiv Fulltext PDF:/Users/emily_ramey/Zotero/storage/UEBV8ESJ/Watts et al. - 2014 - Probing the neutron star interior and the Equation.pdf:application/pdf;arXiv.org Snapshot:/Users/emily_ramey/Zotero/storage/EAUGSKX7/1501.html:text/html}
}

@article{margalit_constraining_2017-1,
	title = {Constraining the {Maximum} {Mass} of {Neutron} {Stars} {From} {Multi}-{Messenger} {Observations} of {GW170817}},
	volume = {850},
	issn = {2041-8213},
	url = {http://arxiv.org/abs/1710.05938},
	doi = {10.3847/2041-8213/aa991c},
	abstract = {We combine electromagnetic (EM) and gravitational wave (GW) information on the binary neutron star (NS) merger GW170817 in order to constrain the radii \$R\_\{{\textbackslash}rm ns\}\$ and maximum mass \$M\_\{{\textbackslash}rm max\}\$ of NSs. GW170817 was followed by a range of EM counterparts, including a weak gamma-ray burst (GRB), kilonova (KN) emission from the radioactive decay of the merger ejecta, and X-ray/radio emission consistent with being the synchrotron afterglow of a more powerful off-axis jet. The type of compact remnant produced in the immediate merger aftermath, and its predicted EM signal, depend sensitively on the high-density NS equation of state (EOS). For a soft EOS which supports a low \$M\_\{{\textbackslash}rm max\}\$, the merger undergoes a prompt collapse accompanied by a small quantity of shock-heated or disk wind ejecta, inconsistent with the large quantity \${\textbackslash}gtrsim 10{\textasciicircum}\{-2\}M\_\{{\textbackslash}odot\}\$ of lanthanide-free ejecta inferred from the KN. On the other hand, if \$M\_\{{\textbackslash}rm max\}\$ is sufficiently large, then the merger product is a rapidly-rotating supramassive NS (SMNS), which must spin-down before collapsing into a black hole. A fraction of the enormous rotational energy necessarily released by the SMNS during this process is transferred to the ejecta, either into the GRB jet (energy \$E\_\{{\textbackslash}rm GRB\}\$) or the KN ejecta (energy \$E\_\{{\textbackslash}rm ej\}\$), also inconsistent with observations. By combining the total binary mass of GW170817 inferred from the GW signal with conservative upper limits on \$E\_\{{\textbackslash}rm GRB\}\$ and \$E\_\{{\textbackslash}rm ej\}\$ from EM observations, we constrain the likelihood probability of a wide-range of previously-allowed EOS. These two constraints delineate an allowed region of the \$M\_\{{\textbackslash}rm max\}-R\_\{{\textbackslash}rm ns\}\$ parameter space, which once marginalized over NS radius places an upper limit of \$M\_\{{\textbackslash}rm max\} {\textbackslash}lesssim 2.17M\_\{{\textbackslash}odot\}\$ (90{\textbackslash}\%), which is tighter or arguably less model-dependent than other current constraints.},
	number = {2},
	urldate = {2020-04-17},
	journal = {The Astrophysical Journal},
	author = {Margalit, Ben and Metzger, Brian},
	month = nov,
	year = {2017},
	note = {arXiv: 1710.05938},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena},
	pages = {L19},
	file = {arXiv Fulltext PDF:/Users/emily_ramey/Zotero/storage/H4D4SKWW/Margalit and Metzger - 2017 - Constraining the Maximum Mass of Neutron Stars Fro.pdf:application/pdf;arXiv.org Snapshot:/Users/emily_ramey/Zotero/storage/8M5E354J/1710.html:text/html}
}

@article{taylor_pulsar_1993,
	title = {Pulsar timing and relativistic gravity},
	volume = {10},
	issn = {0264-9381},
	url = {https://doi.org/10.1088%2F0264-9381%2F10%2Fs%2F017},
	doi = {10.1088/0264-9381/10/S/017},
	abstract = {Measurements of the times of arrival of pulsar signals at Earth provide uniquely powerful tools for experiments in relativistic gravity. The results of such experiments improve rapidly as the span of repeated observations lengthens, and in one instance the relevant data span is now approaching 20 years. The author briefly summarizes and updates two of the most important results of pulsar timing experiments pertinent to relativity and gravitation. The rate of orbital period decay of binary pulsar PSR B1913+16 has been measured with accuracy 0.35\%, and is found to be in excellent accord with the prediction for gravitational radiation damping in general relativity. A more recently discovered binary pulsar system, PSR B1534+12 has provided a unique test of strong-field gravitational effects-and these results, too, are fully consistent with relativity.},
	language = {en},
	number = {S},
