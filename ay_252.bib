	urldate = {2020-04-17},
	journal = {Nature Astronomy},
	author = {Cromartie, H. Thankful and Fonseca, Emmanuel and Ransom, Scott M. and Demorest, Paul B. and Arzoumanian, Zaven and Blumer, Harsha and Brook, Paul R. and DeCesar, Megan E. and Dolch, Timothy and Ellis, Justin A. and Ferdman, Robert D. and Ferrara, Elizabeth C. and Garver-Daniels, Nathaniel and Gentile, Pete A. and Jones, Megan L. and Lam, Michael T. and Lorimer, Duncan R. and Lynch, Ryan S. and McLaughlin, Maura A. and Ng, Cherry and Nice, David J. and Pennucci, Timothy T. and Spiewak, Renee and Stairs, Ingrid H. and Stovall, Kevin and Swiggum, Joseph K. and Zhu, Weiwei},
	month = jan,
