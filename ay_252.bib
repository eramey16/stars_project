}

@article{hobbs_tempo2_2006,
	title = {{TEMPO2}, a new pulsar timing package. {I}: {Overview}},
	volume = {369},
	issn = {00358711},
	shorttitle = {{TEMPO2}, a new pulsar timing package. {I}},
	url = {http://arxiv.org/abs/astro-ph/0603381},
	doi = {10.1111/j.1365-2966.2006.10302.x},
	abstract = {Contemporary pulsar timing experiments have reached a sensitivity level where systematic errors introduced by existing analysis procedures are limiting the achievable science. We have developed tempo2, a new pulsar timing package that contains propagation and other relevant effects implemented at the 1ns level of precision (a factor of {\textasciitilde}100 more precise than previously obtainable). In contrast with earlier timing packages, tempo2 is compliant with the general relativistic framework of the IAU 1991 and 2000 resolutions and hence uses the International Celestial Reference System, Barycentric Coordinate Time and up-to-date precession, nutation and polar motion models. Tempo2 provides a generic and extensible set of tools to aid in the analysis and visualisation of pulsar timing data. We provide an overview of the timing model, its accuracy and differences relative to earlier work. We also present a new scheme for predictive use of the timing model that removes existing processing artifacts by properly modelling the frequency dependence of pulse phase.},
	number = {2},
	urldate = {2020-04-17},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Hobbs, G. and Edwards, R. and Manchester, R.},
	month = jun,
	year = {2006},
	note = {arXiv: astro-ph/0603381},
	keywords = {Astrophysics},
	pages = {655--672}
}

